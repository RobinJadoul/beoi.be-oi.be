---
title: Final Results
trianglify_colors: '"RdYlGn"'
result_file: results_2024_final
---
<section class="page-title page-title-4 image-bg">
  <%=page_header_empty_image_holder %>
  <div class="container">
    <div class="row">
      <div class="col-md-6">
        <h3 class="uppercase mb0"><%= t(:final_result_title) %>&nbsp;<%= results.year %></h3>
      </div>
    </div>
    <!--end of row-->
  </div>
  <!--end of container-->
</section>

<section>
   <div class="container">
      <%=image_tag "archives/2024/beoi24-final.jpg", class:"cast-shadow", width:1512 %>
   </div>
</section>

<section class="altern">
  <div class="container">
    <div class="row">
      <div class="col-sm-8 col-sm-offset-2 text-center">
        <!-- Commenter une des 2 lignes ci-dessous : top-10 à part (proclamation future ) OU classement complet (proclamation passée).
             Modifier aussi les messages dans locales/fr.yml et locales nl.yml -->
        <p class="mb16 text-center lead"><%= t(:final_results_expl_future) %></p>
        <!-- <p class="mb16 text-center lead"><%= t(:final_results_expl_past) %></p> -->
      </div>
    </div>
  </div>
  <!--end of container-->
</section>

<section> <!--</section> class="altern">-->
  <div class="container">
    <div class="row">
      <div class="col-sm-10 col-sm-offset-1 text-center">
        <h4 class="uppercase mb16 text-center"><%= t(:Cadet) %></h4>
      </div>
      <div class="col-sm-12 col-md-10 col-md-offset-1">

        <!-- Ne PAS commenter entre ###Start-top### et ###End-top### pour afficher le top10 à part. -->
        <!-- ###Start-top### -->
        <!-- Attention: data/results_20xy_final.yml doit seulement contenir le top10 en ordre alphabétique et sans points ! -->
        <h5 class="uppercase mb16 text-center"><%= t(:final_top) % results.contestants.cadet_top.length %></h5>

        <table class="table table-striped-cadet">
          <tbody>
            <tr><th><%= t(:column_name) %></th><th><%= t(:column_school) %></th></tr>
            <% results.contestants.cadet_top.each do |r| %>
            <tr>
              <td nowrap="nowrap"><%= r.name %></td>
              <td><%= r.school %> (<%= t(r.year) %>)</td>
            </tr>
            <% end %>
          </tbody>
        </table>

        <h5 class="uppercase mb16 text-center"><%= t(:final_after_top) % results.contestants.cadet_top.length %></h5>
        <!-- ###End-top### -->

        <p class="mb16 text-center"><%= t(:results_max_score)%results.contestants.cadet_max %></p>

        <table class="table table-striped-cadet">
          <tbody>
            <tr><th>#</th><th><%= t(:column_name) %></th><th><%= t(:column_school) %></th><th><%= t(:column_score) %></th></tr>
            <% results.contestants.cadet.each do |r| %>
            <tr>
              <td><%= r.position %></td>
              <td nowrap="nowrap"><%= r.name %></td>
              <td><%= r.school %> (<%= t(r.year) %>)</td>
              <td><strong><%= r.score %></strong></td>
            </tr>
            <% end %>
          </tbody>
        </table>

      </div>
    </div>
    <!--end of row-->

  </div>
  <!--end of container-->
</section>

